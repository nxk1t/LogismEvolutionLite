<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:RGR.Views.Elements">
  <Design.PreviewWith>
    <controls:Element_Not />
  </Design.PreviewWith>

  <Style Selector="controls|Element_Not">
    <!-- Set Defaults -->
    <Setter Property="Template">
      <ControlTemplate>
        <Canvas Width="50" Height="25" Background="Transparent">
          <Ellipse Name="in0" Fill="Gray" Stroke="Black" StrokeThickness="1" Height="10" Width="10" Canvas.Top="8" Canvas.Left="0"/>
          <Polygon Points="10,2 10,23 40,12.5 10,2" Fill="LightGray" Stroke="Black" StrokeThickness="1"/>
          <Ellipse Name="out0" Fill="Gray" Stroke="Black" StrokeThickness="1" Height="10" Width="10" Canvas.Top="8" Canvas.Left="40"/>
        </Canvas>
      </ControlTemplate>
    </Setter>
  </Style>
</Styles>
